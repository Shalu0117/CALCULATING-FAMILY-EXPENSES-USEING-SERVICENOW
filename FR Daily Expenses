    function synchCache() {
      try {
        var w = getTopWindow();
        if (w.g_cache_message)
          w.g_cache_message.stamp("e2302231933322103ed6367f3bba106a");
  
        if (w.g_cache_td)
          w.g_cache_td.stamp("aefd972e93ffe2103ed6367f3bba10a9");
